{{#eq type "heading-one"}}
    <h1 class="element-overskrift">
        {{> aap-saksbehandling-pdfgen/partials/blokkpartial this}}
    </h1>
{{/eq}}
{{#eq type "heading-two"}}
    <h2 class="element-overskrift">
        {{> aap-saksbehandling-pdfgen/partials/blokkpartial this}}
    </h2>
{{/eq}}
{{#eq type "heading-three"}}
    <h3 class="element-overskrift">
        {{> aap-saksbehandling-pdfgen/partials/blokkpartial this}}
    </h3>
{{/eq}}
{{#eq type "heading-four"}}
    <h4 class="element-overskrift">
        {{> aap-saksbehandling-pdfgen/partials/blokkpartial this}}
    </h4>
{{/eq}}
{{#eq type "bulleted-list"}}
    <ul>
        {{#each children as |child|~}}
            <li>
                {{> aap-saksbehandling-pdfgen/partials/blokkpartial this}}
            </li>
        {{/each}}
    </ul>
{{/eq}}
{{#eq type "ordered-list"}}
    <ol>
        {{#each children as |child|~}}
            <li>
                {{> aap-saksbehandling-pdfgen/partials/blokkpartial this}}
            </li>
        {{/each}}
    </ol>
{{/eq}}
{{#eq type "paragraph"}}
    <p>
        {{> aap-saksbehandling-pdfgen/partials/blokkpartial this}}
    </p>
{{/eq}}
